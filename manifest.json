{
  "manifest_version": 2,
  "name": "Todo List",
  "version": "1.0.0",
  "description": "A plain text description",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "popup.html"
  }
}
